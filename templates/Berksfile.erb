# fighter berksfile for <%= style.framework %> <%= style.version %>
# check https://github.com/afaundez/fighter for more options
source 'https://supermarket.chef.io'

<% if techniques = style.techniques['chef_solo'] %>
  <% if cookbooks = techniques['cookbooks'] %>
    <%- cookbooks.each do |cookbook, config| -%>
cookbook '<%= cookbook %>', '<%= config['version'] %>'
    <%- end -%>
  <%- end -%>
<%- end -%>
